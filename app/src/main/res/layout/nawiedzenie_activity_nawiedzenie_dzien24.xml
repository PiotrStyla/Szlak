                    android:id="@+id/textView799"
                    style="@style/paragraf_Text_optimum"/>
            <TextView
                    android:text="@string/codziennie_rano_ko_ci_piewa_pie_zachariasza_i_powraca_w_niej_do_obietnicy_odpuszczenia_grzech_w_codziennie_w_czasie_eucharystii_powtarza_s_owa_chrystusa_to_jest_krew_moja_wylana_na_odpuszczenie_grzech_w_jan_chrzciciel_mia_pom_c_ludziom_uzna_sw_j_grzech_i_przyj_dar_przebaczenia_jest_to_r_wnie_moje_zadanie_sam_potrzebuj_przebaczenia_i_modl_si_o_nie_dla_innych_w_czasie_dziesi_tka_r_a_ca_cz_si_z_innymi_w_tym_wsp_lnym_b_aganiu"
                    android:id="@+id/textView800"
                    style="@style/paragraf_Text_optimum"/>
            <TextView
                    android:text="@string/dziesi_tek_r_a_ca"
                    android:id="@+id/textView801"
                    style="@style/dziesiatek_rozanca"/>

            <TextView
                    android:text="@string/ojcze_nasz_zdrowa_maryjo_chwa_a_ojcu"
                    style="@style/paragraf_Text_optimum"
                    android:id="@+id/textView802"/>
            <TextView
                    android:text="@string/akt_strzelisty"
                    android:id="@+id/textView803"
                    style="@style/intencje_modlitwy"/>
            <TextView
                    android:text="@string/bo_e_w_tr_jcy_wi_tej_jedyny_otw_rz_moje_oczy_abym_m_g_wyra_niej_widzie_wielkie_dzie_a_jakich_dokona_e_w_tajemnicy_nawiedzenia_w_el_biety"
                    style="@style/paragraf_Text_optimum"
                    android:id="@+id/textView804"/>
            <TextView
                    android:text="@string/intencje_modlitwy"
                    android:id="@+id/textView805"
                    style="@style/intencje_modlitwy"/>

            <TextView
                    android:text="@string/intencje_modlitwy_wyra_one_swoimi_s_owami"
                    style="@style/paragraf_Text_optimum"
                    android:id="@+id/textView806"
                    android:textStyle="italic"
                    android:paddingBottom="80sp"/>

        </LinearLayout>
    </ScrollView>
</android.support.constraint.ConstraintLayout>