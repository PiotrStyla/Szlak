                    style="@style/paragraf_Text_optimum"/>
            <TextView
                    android:text="@string/rozwa_anie_s_owa"
                    android:id="@+id/textView4191"
                    style="@style/modlitwa"/>
            <TextView
                    android:text="@string/zgodnie_z_poleceniem_pana_codziennie_powtarzamy_modlitw_ojcze_nasz_przyjd_kr_lestwo_twoje_prosimy_o_jego_przyj_cie_cho_ono_ju_jest_na_tym_wiecie_jest_w_chrystusie_a_wi_c_jest_wsz_dzie_tam_gdzie_chrystus_zosta_przyj_ty_i_gdzie_on_jest_kr_lem_ludzkiego_serca_modlimy_si_aby_chrystus_kt_ry_by_i_kt_ry_jest_nieustannie_przychodzi_i_coraz_bardziej_uobecnia_w_naszym_yciu_kr_lestwo_ojca"
