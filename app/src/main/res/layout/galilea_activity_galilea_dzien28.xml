                    style="@style/paragraf_Text_optimum"/>
            <TextView
                    android:text="@string/dziesi_tek_r_a_ca"
                    android:id="@+id/textView4474"
                    style="@style/dziesiatek_rozanca"/>
            <TextView
                    android:text="@string/w_r_a_cowej_medytacji_przenosz_si_sercem_ku_wszystkim_konfesjona_om_przy_kt_rych_ludzie_wyznaj_swoje_grzechy_i_otrzymuj_przebaczenie_a_tak_e_ku_wszystkim_o_tarzom_na_kt_rych_sprawowana_jest_naj_wi_tsza_ofiara"
