<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginEnd="16dp"
        tools:context=".TaborDzien30"
        android:id="@+id/linearLayout">
    <TextView
            android:text="@string/dzie_30_powr_t_do_wybranych_my_li"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView5027"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            android:lineSpacingExtra="4sp"
            app:layout_constraintTop_toTopOf="parent"
            android:textSize="20sp"
            android:textStyle="bold"
            android:paddingEnd="10sp"
            tools:ignore="RtlSymmetry" />

    <ScrollView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            app:layout_constraintTop_toBottomOf="@+id/textView5027"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="24dp"
            android:layout_marginTop="78dp"
            android:scrollbarStyle="outsideOverlay"
            android:clipToPadding="false"
            android:paddingStart="8sp"
            android:paddingTop="10sp"
            android:paddingEnd="5sp"
            android:paddingBottom="5sp">
        <LinearLayout android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
            <TextView
                    android:text="@string/prosz_boga_abym_mia_mo_liwo_porozmawiania_z_kim_na_temat_wydarze_kt_re_dokona_y_si_na_g_rze_tabor_taka_rozmowa_i_dzielenie_si_prze_yciami_jest_wzajemnym_ubogaceniem"
                    android:id="@+id/textView5028"
                    android:textStyle="italic"
                    style="@style/paragraf_Text_optimum"/>
            <TextView
                    android:text="@string/modlitwa"
                    android:id="@+id/textView5029"
                    style="@style/modlitwa"/>
            <TextView
                    android:text="@string/pro_by_z_jutrzni_wi_ta_przemienienia_pa_skiego"
                    android:id="@+id/textView5030"
                    android:textAllCaps="true"
                    style="@style/paragraf_Text_optimum"
                    android:padding="7dp"/>
            <TextView
                    android:text="@string/z_ufno_ci_zano_my_nasze_pro_by_do_zbawiciela_kt_ry_cudownie_przemieni_si_na_g_rze_w_obecno_ci_swoich_uczni_w"
                    android:id="@+id/textView5031"
                    style="@style/paragraf_Text_optimum"
                    android:padding="7dp"/>
            <TextView
                    android:text="@string/uka_nam_panie_blask_swojej_wiat_o_ci"
                    android:id="@+id/textView5032"
                    style="@style/paragraf_Text_optimum"
                    android:textStyle="italic"
                    android:padding="7dp"/>
            <TextView
                    android:text="@string/ojcze_niesko_czenie_dobry_ty_przemieni_e_swego_mi_owanego_syna_i_objawi_e_si_w_wietlistym_ob_oku_spraw_aby_my_wiernie_s_uchali_s_w_chrystusa"
                    android:id="@+id/textView5033"
                    style="@style/paragraf_Text_optimum"
                    android:padding="7dp"/>
            <TextView
                    android:text="@string/bo_e_ty_hojnie_obdarzy_e_swoich_wybranych_szcz_ciem_i_rado_ci_daj_aby_my_znale_li_w_ciele_chrystusa_r_d_o_naszego_ycia"
                    android:id="@+id/textView5034"
                    style="@style/paragraf_Text_optimum"
                    android:padding="7dp"/>
            <TextView
                    android:text="@string/bo_e_ty_o_wieci_e_nasze_serca_aby_my_ogl_dali_twoj_chwa_na_obliczu_jezusa_chrystusa_pozw_l_nam_zawsze_si_wpatrywa_w_twojego_umi_owanego_syna"
                    android:id="@+id/textView5035"
                    style="@style/paragraf_Text_optimum"
                    android:padding="7dp"/>
            <TextView
                    android:text="@string/bo_e_ty_przez_naszego_zbawiciela_jezusa_chrystusa_objawi_e_nam_powszechny_plan_zbawienia_niech_wiat_o_ewangelii_ogarnie_wszystkich_ludzi"
                    style="@style/paragraf_Text_optimum"
                    android:id="@+id/textView5036"
                    android:padding="7dp"/>
            <TextView
                    android:text="@string/ojcze_dzi_ki_twojej_mi_o_ci_nazywamy_si_dzie_mi_bo_ymi_i_nimi_jeste_my_spraw_aby_my_si_upodobnili_do_chrystusa_gdy_przyjdzie_w_chwale"
                    style="@style/paragraf_Text_optimum"
                    android:id="@+id/textView5037"
                    android:padding="70dp"/>

        </LinearLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>