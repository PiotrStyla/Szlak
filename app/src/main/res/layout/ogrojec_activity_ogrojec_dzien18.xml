                    android:id="@+id/textView5914"
                    style="@style/paragraf_Text_optimum"
                    android:padding="5dp"/>
            <TextView
                    android:text="@string/dziesi_tek_r_a_ca"
                    android:id="@+id/textView5915"
                    style="@style/dziesiatek_rozanca"/>
