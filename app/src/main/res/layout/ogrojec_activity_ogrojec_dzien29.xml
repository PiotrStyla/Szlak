                    style="@style/paragraf_Text_optimum"
                    android:padding="5dp"/>
            <TextView
                    android:text="@string/nadesz_a_godzina_nadszed_czas_dope_nienia_dzie_a_zbawienia_by_em_z_jezusem_w_ogr_jcu_chc_z_nim_by_w_dalszych_chwilach_jego_m_ki"
