            <TextView
                    android:text="@string/m_j_wysi_ek_nawr_cenia_znajduje_swoje_uwie_czenie_w_sakramencie_pokuty_a_moje_przyj_cie_ewangelii_o_kr_lestwie_bo_ym_zwi_zane_jest_przede_wszystkim_z_eucharysti_modl_si_aby_moje_nawr_cenie_by_o_coraz_bardziej_radykalnym_odrzuceniem_ka_dej_my_li_ka_dego_s_owa_i_ka_dego_czynu_kt_re_oddalaj_mnie_od_boga_a_r_wnocze_nie_by_moje_zwr_cenie_si_ku_jezusowi_i_oddanie_si_jemu_by_o_ca_kowite"
