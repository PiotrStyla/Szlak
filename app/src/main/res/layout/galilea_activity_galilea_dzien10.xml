                    android:text="@string/ojcze_nasz_zdrowa_maryjo_chwa_a_ojcu"
                    style="@style/paragraf_Text_optimum"
                    android:id="@+id/textView4197"/>
            <TextView
                    android:text="@string/akt_strzelisty"
                    android:id="@+id/textView4198"
                    style="@style/intencje_modlitwy"/>

            <TextView
                    android:text="@string/objaw_nam_jezu_tajemnic_kr_lestwa_twego_ojca_i_daj_nam_ducha_wi_tego_aby_my_si_nawr_cili"
                    style="@style/paragraf_Text_optimum"
                    android:id="@+id/textView4199"/>
            <TextView
                    android:text="@string/intencje_modlitwy"
                    android:id="@+id/textView4200"
                    style="@style/intencje_modlitwy"/>
            <TextView
                    android:text="@string/intencje_modlitwy_wyra_one_swoimi_s_owami"
                    style="@style/paragraf_Text_optimum"
                    android:id="@+id/textView4201"
