<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginEnd="16dp"
        tools:context=".UkrzyzowanieDzien23"
        android:id="@+id/linearLayout">
    <TextView
            android:text="@string/dzie_23_po_mierci_jezusa_z_o_enie_do_grobu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView7454"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            android:lineSpacingExtra="4sp"
            app:layout_constraintTop_toTopOf="parent"
            android:textSize="20sp"
            android:textStyle="bold"
            android:paddingEnd="10sp"
            tools:ignore="RtlSymmetry" />

    <ScrollView
            android:id="@+id/scrollUkrzyzowanie23"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            app:layout_constraintTop_toBottomOf="@+id/textView7454"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="24dp"
            android:layout_marginTop="78dp"
            android:scrollbarStyle="outsideOverlay"
            android:clipToPadding="false"
            android:paddingStart="8sp"
            android:paddingTop="10sp"
            android:paddingEnd="5sp"
            android:paddingBottom="5sp">
        <LinearLayout android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
            <TextView
                    android:text="@string/modlitwa"
                    android:id="@+id/textView7455"
                    style="@style/modlitwa"/>
            <TextView
                    android:text="@string/s_b_aleksandra_gabrysiak_1993_lekarka_jej_ycie_zwi_zane_by_o_z_tczewem_i_elbl_giem_tam_pe_ni_a_swoj_s_u_b_w_kt_rej_da_a_dobre_wiadectwo_wiary_i_zaanga_owania_w_s_u_bie_ludziom_n_s_b_aleksander_zienkiewicz_1995_prezbiter_pedagog_teolog_duszpasterz_m_odzie_y_akademickiej_rektor_seminarium_duchownego_we_wroc_awiu"
                    android:id="@+id/textView7456"
                    style="@style/paragraf_Text_optimum"
                    android:padding="5dp"/>

            <TextView
                    android:text="@string/rozwa_anie_s_owa"
                    android:id="@+id/textView7457"
                    style="@style/modlitwa"/>
            <TextView
                    android:text="@string/z_ewangelii_wed_ug_w_jana_19_40_42_zabrali_cia_o_jezusa_i_obwi_zali_je_w_p_tna_razem_z_wonno_ciami_stosownie_do_ydowskiego_sposobu_grzebania_a_na_miejscu_gdzie_go_ukrzy_owano_by_ogr_d_w_ogrodzie_za_nowy_gr_b_w_kt_rym_jeszcze_nie_z_o_ono_nikogo_tam_to_wi_c_ze_wzgl_du_na_ydowski_dzie_przygotowania_z_o_ono_jezusa_bo_gr_b_znajdowa_si_w_pobli_u_n_n_chc_dzi_by_szczeg_lnie_blisko_wszystkich_kt_rzy_uczestniczyli_w_z_o_eniu_um_czonego_cia_a_zbawiciela_w_grobie_najpierw_przygotowano_chusty_i_wonno_ci_owini_to_cia_o_i_przeniesiono_do_grobu_potem_zasuni_to_kamie_jezus_zjednoczy_si_z_cz_owiekiem_r_wnie_w_tym_wymiarze_jego_ziemskiego_ycia_po_mierci_zosta_z_o_ony_w_grobie"
                    android:id="@+id/textView7458"
                    style="@style/paragraf_Text_optimum"
                    android:padding="5dp"/>
            <TextView
                    android:text="@string/dziesi_tek_r_a_ca"
                    android:id="@+id/textView7459"
                    style="@style/dziesiatek_rozanca"/>
            <TextView
                    android:text="@string/ojcze_nasz_zdrowa_maryjo_chwa_a_ojcu"
                    style="@style/paragraf_Text_optimum"
                    android:id="@+id/textView7460"/>
            <TextView
                    android:text="@string/akt_strzelisty"
                    android:id="@+id/textView7461"
                    style="@style/intencje_modlitwy"/>
            <TextView
                    android:text="@string/mi_osierny_jezu_przebacz_nam_grzechy_przez_kt_re_ci_krzy_ujemy_daj_ask_nawr_cenia_grzesznikom_i_naucz_nas_sk_ada_swoje_ycie_w_ofierze_za_ludzi"
                    style="@style/paragraf_Text_optimum"
                    android:id="@+id/textView7462"/>
            <TextView
                    android:text="@string/intencje_modlitwy"
                    android:id="@+id/textView7463"
                    style="@style/intencje_modlitwy"/>
            <TextView
                    android:text="@string/intencje_modlitwy_wyra_one_swoimi_s_owami"
                    style="@style/paragraf_Text_optimum"
                    android:id="@+id/textView7464"
                    android:textStyle="italic"
                    android:paddingBottom="70dp"/>
        </LinearLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>