            android:layout_marginTop="78dp"
            android:scrollbarStyle="outsideOverlay"
            android:clipToPadding="false"
            android:paddingStart="8sp"
            android:paddingTop="10sp"
            android:paddingEnd="5sp"
            android:paddingBottom="5sp">
        <LinearLayout android:layout_width="match_parent"
                      android:layout_height="wrap_content"
                      android:orientation="vertical">

            <TextView
                    android:text="@string/rozwa_anie_s_owa"
                    android:id="@+id/textView797"
                    style="@style/rozwazanie_slowa"/>

            <TextView
                    android:text="@string/z_ewangelii_wed_ug_w_ukasza_1_67_76_77_zachariasz_zosta_nape_niony_duchem_wi_tym_i_prorokowa_m_wi_c_a_i_ty_dzieci_zwa_si_b_dziesz_prorokiem_najwy_szego_gdy_p_jdziesz_przed_panem_przygotowa_mu_drogi_jego_ludowi_dasz_pozna_zbawienie_co_si_dokona_przez_odpuszczenie_mu_grzech_w"
