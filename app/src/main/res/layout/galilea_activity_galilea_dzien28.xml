                    style="@style/paragraf_Text_optimum"/>
            <TextView
                    android:text="@string/rozwa_anie_s_owa"
                    android:id="@+id/textView4469"
                    style="@style/modlitwa"/>
            <TextView
                    android:text="@string/z_ewangelii_wed_ug_w_ukasza_8_1_jezus_w_drowa_przez_miasta_i_wsie_nauczaj_c_i_g_osz_c_ewangeli_o_kr_lestwie_bo_ym"
                    android:id="@+id/textView4470"
                    style="@style/paragraf_Text_optimum"/>
            <TextView
                    android:text="@string/jezus_dalej_w_druje_przez_miasta_i_wsie_docieraj_c_przez_swoich_uczni_w_a_do_kra_c_w_ziemi_on_w_druje_r_wnie_z_cz_owiekiem_przez_kolejne_lata_jego_ycia_towarzyszy_mu_w_dzieci_stwie_i_m_odo_ci_w_wieku_dojrza_ym_i_w_trudnie_staro_ci_we_wszystkich_miejscach_ziemi_i_w_ka_dym_okresie_ycia_cz_owieka_g_osi_ewangeli_o_kr_lestwie_i_wzywa_ich_do_nawr_cenia"
